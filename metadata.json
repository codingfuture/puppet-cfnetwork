{
  "name": "codingfuture-cfnetwork",
  "version": "0.9.2",
  "author": "Andrey Galkin",
  "summary": "Resource-based network interface and firewall configuration (firewall provider is separate module)",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfnetwork",
  "project_page": "https://github.com/codingfuture/puppet-cfnetwork",
  "issues_url": "https://github.com/codingfuture/puppet-cfnetwork/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "15.04",
        "15.10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.11.0"
    },
    {
      "name": "fiddyspence-sysctl",
      "version_requirement": ">= 1.1.0"
    }
  ],
  "data_provider": "hiera"
}
